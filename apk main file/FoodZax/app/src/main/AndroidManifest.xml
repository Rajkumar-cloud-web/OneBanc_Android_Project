<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.foodzax">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FoodZax">
        <activity android:name=".Cart"></activity>
        <activity android:name=".SouthIndianFood" />
        <activity android:name=".MediterraneanFood" />
        <activity android:name=".ItalianFood" />
        <activity android:name=".ChineseFood" />
        <activity android:name=".NorthIndianFood" />
        <activity android:name=".All_Dishes" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>